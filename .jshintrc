// .jshintrc
// Global configuration defaults for the jshint tool.
{
  "node": true,
  "browser": false,
  "esnext": true,
  "bitwise": true,
  "camelcase": false,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "regexp": true,
  "undef": true,
  "strict": true,
  "trailing": true,
  "smarttabs": true,
  "expr": true,
  "globals": {
    "_": true,
    "after": false,
    "afterEach": false,
    "before": false,
    "beforeEach": false,
    "describe": false,
    "expect": false,
    "it": false,
    "Q": true,
    "spyon": false,
    "xdescribe": false,
    "xit": false
  }
}
